{
  "name": "@ycore/foundry",
  "version": "1.0.3",
  "private": true,
  "type": "module",
  "exports": {
    "./auth/components": {
      "development": {
        "import": "./src/auth/components/index.ts"
      },
      "types": "./lib/auth/components/index.d.ts",
      "import": "./lib/auth/components/index.js"
    },
    "./auth/config": {
      "development": {
        "import": "./src/auth/config/index.ts"
      },
      "types": "./lib/auth/config/index.d.ts",
      "import": "./lib/auth/config/index.js"
    },
    "./auth/pages": {
      "development": {
        "import": "./src/auth/pages/index.ts"
      },
      "types": "./lib/auth/pages/index.d.ts",
      "import": "./lib/auth/pages/index.js"
    },
    "./auth/routes": {
      "development": {
        "import": "./src/auth/routes.ts"
      },
      "types": "./lib/auth/routes.d.ts",
      "import": "./lib/auth/routes.js"
    },
    "./auth/schema": {
      "development": {
        "import": "./src/auth/config/db/schema.ts",
        "require": "./src/auth/config/db/schema.js"
      },
      "types": "./lib/auth/config/db/schema.d.ts",
      "import": "./lib/auth/config/db/schema.js",
      "require": "./lib/auth/config/db/schema.js"
    },
    "./auth/server": {
      "development": {
        "import": "./src/auth/server/index.ts"
      },
      "types": "./lib/auth/server/index.d.ts",
      "import": "./lib/auth/server/index.js"
    },
    "./common": {
      "development": {
        "import": "./src/common/index.ts"
      },
      "types": "./lib/common/index.d.ts",
      "import": "./lib/common/index.js"
    },
    "./email": {
      "development": {
        "import": "./src/email/index.ts"
      },
      "types": "./lib/email/index.d.ts",
      "import": "./lib/email/index.js"
    },
    "./form": {
      "development": {
        "import": "./src/form/index.ts"
      },
      "types": "./lib/form/index.d.ts",
      "import": "./lib/form/index.js"
    },
    "./form/server": {
      "development": {
        "import": "./src/form/server/index.ts"
      },
      "types": "./lib/form/server/index.d.ts",
      "import": "./lib/form/server/index.js"
    },
    "./vendor": {
      "development": {
        "import": "./src/vendor/index.ts"
      },
      "types": "./lib/vendor/index.d.ts",
      "import": "./lib/vendor/index.js"
    }
  },
  "dependencies": {
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@oslojs/otp": "^1.1.0",
    "@tsndr/cloudflare-worker-jwt": "^3.1.3",
    "confbox": "^0.1.8",
    "jose": "^5.8.0",
    "remix-auth": "^4.1.0",
    "remix-auth-form": "^3.0.0",
    "remix-toast": "^2.0.0",
    "remix-utils": "^8.0.0",
    "sonner": "^1.7.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "table": "^6.9.0",
    "tsx": "^4.19.2",
    "wrangler": "^3.95.0"
  },
  "peerDependencies": {
    "@react-router/cloudflare": ">=7.0.0",
    "@ycore/componentry": ">=1.0.0",
    "drizzle-orm": ">=0.36.0",
    "react-router": ">=7.0.0"
  },
  "scripts": {
    "build": "tsc --build ./tsconfig.workspace.json"
  },
  "bin": {
    "cf-sync": "./sync/cf-sync.ts",
    "init-routes": "./scripts/init-routes"
  }
}
